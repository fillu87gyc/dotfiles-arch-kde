	"vim.overrideCopy": false,
	"vim.useSystemClipboard": true,
	"vim.easymotion": true,
	"vim.easymotionKeys": "qazwsxedcrfvtgbyhnujmikol",
	"git.ignoreMissingGitWarning": true,
	"editor.minimap.enabled": false,
	"editor.quickSuggestions": {
		"other": true,
		"comments": true,
		"strings": true
	},
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.renderWhitespace": "selection",
	"[ruby]": {
		"editor.autoIndent": "none",
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[erb]": {
		"editor.autoIndent": true,
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"git.autofetch": true,
	"markdownlint.config": {
		"MD009": {
			"br_spaces": 2
		}
	},
	"workbench.colorTheme": "Monokai",
	"editor.renderIndentGuides": true,
	"editor.fontSize": 16,
	"vsicons.dontShowNewVersionMessage": true,
	"terminal.external.windowsExec": "",
	"java.semanticHighlighting.enabled": true,
	"java.format.settings.url": "/home/fill/.vscode-oss/extensions/redhat.java/eclipse-formatter.xml",
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"[css]": {
		"editor.suggest.insertMode": "replace"
	},
	"java.home": "/usr/lib/jvm/java-11-openjdk",
	"latex-workshop.view.pdf.viewer": "tab",
	// "latex-workshop.latexindent.path": "/usr/bin/latexindent",
	"latex-workshop.latex.recipes": [
		{
			"name": "compile",
			"tools": [
				"ptex2pdf"
			]
		}
	],
	"latex-workshop.latex.tools": [
		{
			"name": "ptex2pdf",
			"command": "bash",
			"args": [
				"%DIR%compile.sh",
			]
		}
	],
	"latex-workshop.latex.autoBuild.run": "onFileChange",
	"latex-workshop.docker.enabled": true,
	"[python]": {
		"editor.tabSize": 4,
		"editor.insertSpaces": true
	},
	"terminal.integrated.allowMnemonics": true,
	"editor.wordWrap": "on",
	"python.dataScience.sendSelectionToInteractiveWindow": true,
	"python.pythonPath": "/usr/bin/python3",
	"terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
	"terminal.integrated.allowChords": false,
	"docker.images.sortBy": "CreatedTime",
	"git.enableSmartCommit": true
}