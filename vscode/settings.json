{
	"editor.rulers": [
		80,
		100
	],
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"editor.formatOnSave": true,
	"editor.formatOnSaveTimeout": 1000,
	"window.zoomLevel": 1,
	"files.associations": {
		"*.html": "html",
		"*.erb": "erb",
		".vsvimrc": "viml",
		"新年": "markdown"
	},
	"vim.insertModeKeyBindings": [
		{
			"before": [
				"j",
				"j"
			],
			"after": [
				"<Esc>"
			]
		}
	],
	"vim.leader": "<space>",
	"vim.normalModeKeyBindings": [
		{
			"before": [
				"leader",
				"l"
			],
			"after": [
				"$"
			]
		},
		{
			"before": [
				"leader",
				"h"
			],
			"after": [
				"^"
			]
		},
